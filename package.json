{
  "name": "test-inputs",
  "version": "1.3.0",
  "description": "Easily get comprehensive lists of all data types, including lesser-known edge cases",
  "author": "Reid Moffat <reid.moffat9@gmail.com>",
  "license": "MIT",
  "keywords": [
    "test",
    "testing",
    "test-data",
    "input",
    "inputs",
    "data",
    "types",
    "coverage",
    "test-coverage",
    "unit-testing",
    "qa",
    "mock",
    "mock-data",
    "edge-cases",
    "comprehensive",
    "easy",
    "lightweight",
    "typescript"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/index.js",
  "jsdelivr": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "lint": "tsc",
    "build": "tsup src/index.ts --format cjs,esm --dts --minify --clean",
    "test": "cross-env TS_NODE_PROJECT='./tsconfig.json' mocha",
    "deployHelp": "echo 1) Run 'changeset' 2) Merge changes to main 3) Merge changeset PR 4) npm run deploy (verify it looks good)",
    "deploy": "git checkout main && git pull && npm run build && npm publish"
  },
  "files": [
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "dist"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.1.0",
    "bias-random": "^1.1.0",
    "chai": "^5.2.1",
    "cross-env": "^10.0.0",
    "generate-arrays": "^2.0.0",
    "mocha": "^11.7.1",
    "suite-metrics": "^2.4.0",
    "time-range-utils": "^1.0.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reid-moffat/test-inputs"
  },
  "homepage": "https://www.npmjs.com/package/test-inputs?activeTab=readme",
  "bugs": "https://github.com/reid-moffat/test-inputs/issues",
  "funding": {
    "type": "buymeacoffee",
    "url": "https://buymeacoffee.com/reidmoffat"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false,
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "microtime"
    ]
  },
  "packageManager": "pnpm@10.18.3"
}
